public class ReverseCharSequence implements CharSequence{
    private str = "";
    public ReverseCharSequence(String a) {
	for (int i = a.length()-1; i >= 0; a--) {
	    str += a.charAt(i);
	}
    }
    public int CharAt(int index) {}
    
    public int length() {}
    
    public String subSequence(int start, int end) {}
